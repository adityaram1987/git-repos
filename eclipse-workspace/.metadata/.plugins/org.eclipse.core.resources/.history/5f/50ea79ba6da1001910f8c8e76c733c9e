package com.pack.mockitodemo;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

public class TestCalculator {

	Calculator c1= null;
	@Test
	public void test() {
		assertTrue(c1.performCalculation(cs));
	}
	
	@Before
